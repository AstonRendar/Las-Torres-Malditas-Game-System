{
  "id": "las-torres-malditas",
  "title": "Las Torres Malditas",
  "description": "Sistema de rol completo para Foundry VTT, con ambientación propia, mecánica original y soporte completo de lore, talentos y enemigos.",
  "version": "0.1.0",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "José Enrique Labordeta"
    }
  ],
  "scripts": [
    "scripts/main.js"
  ],
  "esmodules": [
    "scripts/main.js"
  ],
  "styles": [
    "styles/system.css"
  ],
  "packs": [],
  "languages": [
    {
      "lang": "es",
      "name": "Español",
      "path": "lang/es.json"
    }
